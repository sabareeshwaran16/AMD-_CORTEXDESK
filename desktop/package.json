{
  "name": "cortexdesk-desktop",
  "version": "1.0.0",
  "description": "CortexDesk Desktop Application",
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron:build\" \"npm run dev:electron:run\"",
    "dev:react": "vite",
    "dev:electron:build": "tsc -p electron/tsconfig.json --watch",
    "dev:electron:run": "wait-on electron/dist/main.js && wait-on http://localhost:5173 && electron electron/dist/main.js",
    "build": "tsc && vite build && electron-builder",
    "build:react": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "preview": "vite preview"
  },
  "keywords": [
    "desktop",
    "electron",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}

